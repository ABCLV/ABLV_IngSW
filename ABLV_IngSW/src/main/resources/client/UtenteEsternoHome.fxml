<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/21"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="client.UtenteEsternoHomeController"
      alignment="CENTER"
      spacing="15"
      prefWidth="400"
      prefHeight="300"
      style="-fx-background-color: #e6f7ff;">

    <Label text="Ricerche"
           style="-fx-font-size: 22; -fx-font-weight: bold; -fx-text-fill: #005f5f;" />

    <Button text="Gare" onAction="#handleGare" maxWidth="250"/>
    <Button text="Concorrenti" onAction="#handleConcorrenti" maxWidth="250"/>
    <Button text="Campo Gara" onAction="#handleCampoGara" maxWidth="250"/>
    <Button text="Indietro" onAction="#handleBack" maxWidth="250"/>
</VBox>
